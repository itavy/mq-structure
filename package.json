{
  "name": "@itavy/mq-structure",
  "version": "0.0.1",
  "description": "Mq structure",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test:check-style": "./node_modules/.bin/eslint index.js \"lib/latest/*.js\" \"test/**/*.js\"",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "docs-md": "./node_modules/.bin/jsdoc2md --template README.hbs lib/latest/*.js > README.md",
    "pretest": "npm run test:check-style"
  },
  "pre-commit": [
    "test:coverage"
  ],
  "keywords": [
    "mq",
    "mq serialization",
    "mq structure"
  ],
  "author": "Octavian Ionescu <itavyg@gmail.com> (https://github.com/itavy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itavy/mq-structure/issues"
  },
  "homepage": "https://github.com/itavy/mq-structure#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itavy/mq-structure.git"
  },
  "dependencies": {
    "@itavy/extended-map": "~4.0.0",
    "@itavy/ierror": "~1.2.0",
    "@itavy/utilities": "~1.1.1",
    "debug": "~4.1.1",
    "protobufjs": "~6.8.8",
    "semver": "~5.6.0"
  },
  "devDependencies": {
    "@itavy/test-utilities": "~6.0.0",
    "eslint": "~5.12.1",
    "eslint-config-airbnb": "~17.1.0",
    "eslint-plugin-import": "~2.15.0",
    "eslint-plugin-jsx-a11y": "~6.2.0",
    "eslint-plugin-react": "~7.12.4",
    "jaguarjs-jsdoc": "~1.1.0",
    "jsdoc": "~3.5.5",
    "jsdoc-to-markdown": "~4.0.1",
    "mocha": "~5.2.0",
    "nyc": "~13.1.0",
    "pre-commit": "~1.2.2",
    "tap": "~12.4.0"
  },
  "nyc": {
    "all": true,
    "color:": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "lib/latest/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "watermarks": {
      "check-coverage": true,
      "statements": [
        50,
        95
      ],
      "lines": [
        50,
        95
      ],
      "functions": [
        50,
        95
      ],
      "branches": [
        50,
        95
      ]
    }
  }
}
